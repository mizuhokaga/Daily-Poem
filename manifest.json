{
    "manifest_version": 3,
    "name": "poem",
    "version": "1.0.0",
    "description": "A plugin about poem",
    "author": "mizuhokaga",
    "action": {
        "default_title": "Daily Poem",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon.png",
            "24": "images/icon.png",
            "32": "images/icon.png"
        }
    },
    "host_permissions": [
        "https://v2.jinrishici.com/one.json",
        "https://*.baidu.com/*"
    ]

}